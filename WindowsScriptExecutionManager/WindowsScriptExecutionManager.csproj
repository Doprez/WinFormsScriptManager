<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <DebugType>embedded</DebugType>
	<VersionPrefix>0.0.0.1</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\Icons\icons8-file-96.png" />
    <None Remove="Resources\Images\Icons\icons8-powershell-96.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\Images\Icons\icons8-file-96.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\Images\Icons\icons8-powershell-96.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

</Project>